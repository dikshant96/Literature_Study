pages = {583--589},
publisher = {American Society of Mechanical Engineers Digital Collection},
title = {{A Two-Equation Model for Heat Transport in Wall Turbulent Shear Flows}},
url = {https://asmedigitalcollection.asme.org/heattransfer/article/110/3/583/382763/A-TwoEquation-Model-for-Heat-Transport-in-Wall},
volume = {110},
