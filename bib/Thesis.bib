@article{Nagano1988,
abstract = {{\textless}p{\textgreater}A new proposal for closing the energy equation is presented at the two-equation level of turbulence modeling. The eddy diffusivity concept is used in modeling. However, just as the eddy viscosity is determined from solutions of the k and $\epsilon$ equations, so the eddy diffusivity for heat is given as functions of temperature variance t2, and the dissipation rate of temperature fluctuations $\epsilon$t, together with k and $\epsilon$. Thus, the proposed model does not require any questionable assumptions for the “turbulent Prandtl number.” Modeled forms of the t2 and $\epsilon$t equations are developed to account for the physical effects of molecular Prandtl number and near-wall turbulence. The model is tested by application to a flat-plate boundary layer, the thermal entrance region of a pipe, and the turbulent heat transfer in fluids over a wide range of the Prandtl number. Agreement with the experiment is generally very satisfactory.{\textless}/p{\textgreater}},
author = {Nagano, Y. and Kim, C.},
doi = {10.1115/1.3250532},
file = {:home/dikshant/Desktop/Thesis{\_}Literature/Papers/Nagano, Kim - 1988 - A Two-Equation Model for Heat Transport in Wall Turbulent Shear Flows.pdf:pdf},
issn = {0022-1481},
journal = {Journal of Heat Transfer},
keywords = {Boundary layers,Eddies (Fluid dynamics),Energy dissipation,Entrance region,Flat plates,Fluctuations (Physics),Fluids,Forced convection,Heat,Modeling,Pipes,Prandtl number,Shear turbulence,Temperature,Turbulence,Turbulent heat transfer,Viscosity},
month = {aug},
